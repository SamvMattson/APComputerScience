import java.util.*;

public class TestClass {
	
	public static void main(String[] args) {
		
		String name = "asdfg";
		String temp = "";
		String output = "";
		
		for (int i = name.length(); i > 0; i--) {
			temp = name.substring(name.length() - 1);
			output += temp;
		}
		
		System.out.println(temp);
		
	}
	
}
